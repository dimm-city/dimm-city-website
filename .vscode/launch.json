{
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "chrome",
      "request": "launch",
      "name": "dc - chrome",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}",
      "preLaunchTask": "npm run dev",
    },
        {
          "name": "Launch server",
          "request": "launch",
          "runtimeArgs": [
            "run-script",
            "dev"
          ],
          "runtimeExecutable": "npm",
          "skipFiles": [
            "<node_internals>/**"
          ],
          "type": "node",
          "console": "integratedTerminal"
        }
  ]
}